    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Do Not Update</fullName>
                <default>false</default>
                <label>Do Not Update</label>
            </value>
            <value>
                <fullName>Report Duplicate</fullName>
